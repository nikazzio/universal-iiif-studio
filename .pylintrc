[MASTER]
ignore=.venv,downloads,logs,models,temp_images,__pycache__,.ruff_cache,.pytest_cache

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=
    line-too-long,
    trailing-whitespace,
    missing-module-docstring,
    missing-class-docstring,
    missing-function-docstring,
    too-many-arguments,
    too-many-positional-arguments,
    too-many-instance-attributes,
    too-many-locals,
    too-many-branches,
    too-many-statements,
    too-many-return-statements,
    too-many-nested-blocks,
    too-few-public-methods,
    import-outside-toplevel,
